using System;

class Program
{
    static void Main()
    {
        // Описание вещественных переменных a, b, c
        double a = 12.3;
        double b = 45.8;
        double c = 78.9;

        // Вычисление среднего арифметического
        double sred = (a + b + c) / 3;

        // Вывод результата на экран
        Console.WriteLine("Среднее арифметическое вещественных чисел: " + sred);

   // Описание целочисленных переменных a, b, c
        int a2 = 12;
        int b2 = 46;
        int c2 = 79;

        // Вычисление среднего арифметического
        int sred2 = (a2 + b2 + c2) / 3;

        // Вывод результата на экран
        Console.WriteLine("Среднее арифметическое целых чисел: " + sred2);
    }
}
